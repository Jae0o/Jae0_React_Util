{
  "name": "@jae0/util",
  "version": "0.0.3",
  "description": "These are various utils used in typescript and javascript.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "npm run cache && npm run esbuild && npm run tsc",
    "tsc": "npx tsc --emitDeclarationOnly",
    "esbuild": "node esbuild.config.js",
    "cache": "rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "utils",
    "react",
    "typescript",
    "type",
    "javascript",
    "haversine",
    "haversineFormula"
  ],
  "devDependencies": {
    "@types/node": "^20.12.12",
    "esbuild": "^0.21.3",
    "typescript": "^5.4.5"
  },
